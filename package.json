{
  "type": "module",
  "name": "pwa",
  "version": "1.0.0",
  "description": "@cmd minor web",
  "main": "app.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "nodemon --exec babel-node src/scripts/main.js",
    "start": "npm run build && babel-node ./build/index.js",
    "build": "npm run clean && npm run build-babel",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RickGroot/web-app-from-scratch-2021.git"
  },
  "author": "Rick Groot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RickGroot/web-app-from-scratch-2021/issues"
  },
  "homepage": "https://github.com/RickGroot/web-app-from-scratch-2021#readme",
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "cross-fetch": "^3.0.6",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-async-await": "^1.1.0",
    "node-fetch": "^2.6.1",
    "node-localstorage": "^2.1.6",
    "nodejs": "0.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "babel-cli": "^6.26.0",
    "nodemon": "^2.0.7"
  },
  "browserslist": [
    "since 2017-06"
  ]
}
